{
  "all": true,
  "include": ["src/**/*.ts"],
  "exclude": ["src/test/**", "**/*.spec.ts", "**/*.test.ts", "**/generated-types.ts"],
  "reporter": ["text", "html", "lcov"],
  "check-coverage": true,
  "lines": 80,
  "functions": 75,
  "branches": 70,
  "statements": 80,
  "per-file": false,
  "skip-full": false
}
